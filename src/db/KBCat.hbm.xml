<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!--
 * Hibernate mapping file for "KBCat" class.
 * 
 * @author Alois Seckar [ ellrohir@seznam.cz ]
 * @version 0.1
 * @since 2015-02-13 16:04 GMT
-->
<hibernate-mapping package="db">
  <class name="KBCat">
    <id column="id" name="catID" type="int">
      <generator class="native"/>
    </id>
    <property column="ord" name="catOrd" type="int"/>
    <property column="name" length="25" name="catName" type="string"/>
  </class>
</hibernate-mapping>
